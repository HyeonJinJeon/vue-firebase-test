ion: 'users',\n      docId: this.$route.params.id,\n      rows: {},\n      row: {\n        modifyName: '',\n        modifyAge: 0,\n        modifyGender: true,\n        modifySchool: '',\n        modifySchoolNum: 0,\n        modifyPhoneNum: '',\n      }\n\n    }\n  },\n  methods: {\n    getData() {\n      const self = this;\n      const db = firebase.firestore();\n      db.collection(self.fbCollection)\n          .doc(this.docId)\n          .get()\n          .then((snapshot) => {\n            const _data = snapshot.data();\n            this.rows = _data\n          })\n    },\n    modifyData() {\n      const self = this;\n      const db = firebase.firestore();\n      db.collection(self.fbCollection)\n          .doc(this.docId)\n          .update({\n            name: self.row.modifyName,\n            age: self.row.modifyAge,\n            gender: self.row.modifyGender,\n            school: self.row.modifySchool,\n            schoolNum: self.row.modifySchoolNum,\n            phoneNum: self.row.modifyPhoneNum,\n          })\n          .then(() => {\n            self.$router.push('/')\n          })\n    },\n    init() {\n      this.getData()\n    },\n  },\n  mounted() {\n    const self = this;\n    self.init();\n  },\n  comments: {},\n  props: {},\n}\n</script>\n\n"]}]}