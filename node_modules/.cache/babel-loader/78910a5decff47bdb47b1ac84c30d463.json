v>\n  </div>\n</template>\n\n\n<script>\n// @ is an alias to /src\n\n\nimport {firebase} from '@/firebase/firebaseConfig';\n\nexport default {\n  name: 'EachUserData',\n  components: {\n\n  },\n  data(){\n    return {\n      fbCollection: 'users',\n      docId: this.$route.params.id,\n      rows: [],\n    }\n  },\n  methods: {\n    getData(){\n      const self = this;\n      const db = firebase.firestore();\n      db.collection(self.fbCollection)\n          .doc(this.docId)\n          .get()\n          .then((snapshot)  => {\n            const _data = snapshot.data();\n            this.rows = _data\n          })\n    },\n    init(){\n      this.getData()\n    },\n    modifyUser() {\n      this.$router.push({name: 'modifyuserdata', params: {id: this.docId}}).catch(()=>{});\n    },\n    deleteUser() {\n      const self = this;\n      const db = firebase.firestore();\n      db.collection(self.fbCollection)\n          .doc(this.docId)\n          .delete()\n          .then(()  => {\n            self.$router.push('/')\n          })\n    },\n  },\n  mounted() {\n    const self = this;\n    self.init();\n  },\n  comments: {\n\n  },\n  props : {\n\n  },\n}\n</script>\n"]}]}